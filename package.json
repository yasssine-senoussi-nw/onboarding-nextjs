{
  "name": "nw-nextjs-boilerplate",
  "private": true,
  "packageManager": "pnpm@9.1.1+sha256.9551e803dcb7a1839fdf5416153a844060c7bce013218ce823410532504ac10b",
  "engines": {
    "pnpm": ">=9.0.0",
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "pnpm check-filesize && next dev",
    "build": "next build --no-lint",
    "build:export": "NEXT_EXPORT_BUILD=true next build --no-lint",
    "build:standalone": "NEXT_STANDALONE_BUILD=true next build --no-lint",
    "start": "next start",
    "lint:target": "next lint --ignore-path .gitignore --max-warnings 0",
    "lint": "pnpm lint:target --dir .",
    "lint:all": "pnpm lint",
    "lint:fix": "pnpm lint:all --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:snap": "jest --maxWorkers=50% --silent --watchAll=false --testMatch='!**/*' --testMatch='**/*.snap.tsx'",
    "test:coverage": "jest --coverage --maxWorkers=50% --silent --watchAll=false --reporters=default --coverageReporters=cobertura --coverageReporters=text-summary --coverageReporters=json-summary --coverageReporters=lcov --testMatch='**/*.{spec,test}.{js,jsx,ts,tsx}'",
    "generate": "plop",
    "preinstall": "npx -y only-allow pnpm",
    "prepare": "next telemetry disable && husky install",
    "check-filesize": "node .ci/scripts/check-filesize.js",
    "pre-push-check": "pnpm lint && pnpm test && pnpm build",
    "pre-commit-check": "lint-staged && pnpm check-filesize && pnpm knip",
    "release": "release-it --config config/release.config.js --only-version",
    "commit-message-check": "commitlint --edit",
    "knip": "knip --config config/knip.config.js"
  },
  "dependencies": {
    "@emotion/cache": "^11.10.5",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/material": "^5.10.15",
    "@reduxjs/toolkit": "^1.9.2",
    "axios": "^0.27.2",
    "i18next": "^22.4.14",
    "i18next-browser-languagedetector": "^7.0.1",
    "next": "14.2.4",
    "next-secure-headers": "^2.2.0",
    "notistack": "^2.0.8",
    "query-string": "^7.1.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-i18next": "^12.2.0",
    "react-query": "^3.39.0",
    "react-redux": "^8.0.5",
    "sharp": "^0.33.4",
    "zod": "^3.24.2",
    "zod-storage": "^0.0.2"
  },
  "prettier": {
    "endOfLine": "lf",
    "printWidth": 120,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "all"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.4",
    "@commitlint/config-conventional": "^18.4.4",
    "@emotion/jest": "^11.10.5",
    "@nimbleways/eslint-config": "^0.3.0",
    "@release-it/conventional-changelog": "^8.0.1",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.7",
    "@testing-library/user-event": "^14.5.2",
    "@total-typescript/ts-reset": "^0.4.2",
    "@tsconfig/next": "^1.0.5",
    "@tsconfig/strictest": "^2.0.1",
    "@types/eslint": "^8.4.10",
    "@types/jest": "^29.4.0",
    "@types/node": "18.11.18",
    "@types/react": "18.0.27",
    "@types/react-redux": "^7.1.25",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.40.0",
    "eslint-config-next": "14.2.4",
    "glob-all": "^3.3.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "knip": "^5.16.0",
    "lint-staged": "^14.0.0",
    "next-http-proxy-middleware": "^1.2.5",
    "plop": "^3.1.1",
    "postcss": "^8.4.21",
    "prettier": "^3.0.0",
    "release-it": "^17.2.1",
    "tailwindcss": "^3.2.4",
    "typescript": "5.0.4"
  },
  "resolutions": {
    "@typescript-eslint/parser": "6.2.1"
  }
}
